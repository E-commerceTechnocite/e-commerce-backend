{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/(.*)": "<rootDir>/../src/$1",
    "@app/e2e/(.*)": "<rootDir>/$1",
    "@app/console/(.*)": "<rootDir>/../libs/console/src/$1",
    "@app/console": "<rootDir>/../libs/console/src",
    "@app/release-console/(.*)": "<rootDir>/../libs/release-console/src/$1",
    "@app/release-console": "<rootDir>/../libs/release-console/src"
  }
}